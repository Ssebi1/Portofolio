<!DOCTYPE html>
<html>

<head>
	<script src="https://use.fontawesome.com/35e0e0136a.js"></script>
	<!-- Seo -->
	<meta name="keywords"
		content="HTML, CSS, JavaScript, Dancau Sebastian,Sebastian Dancau,sebastian dancau,dancau sebastian, Cv, Resume, Internship, Job, Dancau, Sebastian, personal website">
	<meta name="description"
		content="Dancau Sebastian's personal website. Here are available projects, resume and details about me.">
	<meta name="author" content="Dancau Sebastian">
	<meta name="language" content="en">

	<link rel="shortcut icon" href="/favicon.ico?v=1.1">
	<link rel="apple-touch-icon" href="../public/apple_icon.png" />

	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Dăncău Sebastian - Personal Website</title>
	<link rel="stylesheet" type="text/css" href="index.css">
	<link rel="stylesheet" href="https://unpkg.com/tippy.js@6/animations/shift-toward.css" />
	<link rel="stylesheet" href="https://unpkg.com/tippy.js@6/themes/light-border.css" />
	<meta name="google-site-verification" content="2BOyB7vYQHeEDGq2JnB9oT_c6v5PiueojTtvbEiUskE" />
</head>

<body>
	<script src="https://unpkg.com/@popperjs/core@2"></script>
	<script src="https://unpkg.com/tippy.js@6"></script>
	<div class="main">

		<!-- Seo -->
		<h1 style="display: none;">Dancau Sebastian Personal Website</h1>
		<h2 style="display: none;">Personal Website</h2>


		<!-- Hamburger icon -->
		<%- include('modules/hamburger') %>

		<!-- Left area -->
		<div class="leftside">

			<!-- Profile image, name and subtitle -->
			<%- include('modules/profileShowcase',{
				profileCountry: 'IN ROMANIA'
			}) %>

			<!-- Social icons -->
			<p style="display: none;">Social Pages</p>
			<%- include('modules/social') %>

			<!-- Navigation -->
			<p style="display: none;">Navigation</p>
			<%- include('modules/navigation',{
					navigationItem: ['Home','About','Education','Experience','Skills','Projects','Training','Achivements','Contact']
			}) %>

			<!-- Footer -->
			<%- include('modules/footer',{
				footerContent: 'Copyright© 2020 All rights reserved<br>Developed by <b>Dancau Sebastian</b>'
			}) %>



			<!-- Language selection -->
			<%- include('modules/languages',{
				language: 'en'
			}) %>

		</div>


		<!-- Right area -->
		<div class="rightside">

			<!-- Banner image and text -->
			<%- include('modules/banner',{
				bannerTitle: "Hi!<br>I'm Sebi",
				subTitle: 'And I'm a passionate developer.',
				buttonText: 'DOWNLOAD CV'
			}) %>


			<!-- About area -->
			<%- include('modules/aboutArea',{
				<!-- Small title goes here -->
				smallTitle: 'About me',

				<!-- Big title goes here -->
				bigTitle: 'Who am i?',

				<!-- Content goes here -->
				aboutContent: "I am a motivated and hardworking individual with great passion for coding and developing websites. I am currently in my first year of studying Computer Science at university. Last years i've worked on multiple projects, personal or paid and i achieved some of the programming skills necessary nowadays.<br><br>Anytime I have the opportunity to learn something new, I won't miss it. I am currently available for a job opportunity and searching for a summer internship experience. Some things about my projects: fast, responsive, attractive, dynamic."
			}) %>


			<!-- Education area -->
			<%- include('modules/educationArea',{
				<!-- Small title goes here -->
				smallTitle: 'Education history',

				<!-- Big title goes here -->
				bigTitle: 'Education',

				<!-- Education tiles goes here -->
				education: [
				'2023-present</span><br> University of Bucharest, Computer Science - Master\'s degree',
				'2020-2023</span><br> University of Bucharest, Computer Science - Bachelor\'s degree',
				'2016-2020</span><br> Highschool "Mircea Cel Batran", Rm. Valcea<br>Mathematics and computer science',
				]
			}) %>

			<!-- Experience area -->
			<%- include('modules/experienceArea',{
				<!-- Small title goes here -->
				smallTitle: 'Experience',

				<!-- Big title goes here -->
				bigTitle: 'My work experience',

				<!-- Achivement tiles goes here -->
				experience: [
				{
					'logo': './Images/sst.png',
					'company': 'Stone Soup Tech',
					'period':'july 2021 - present',
					'position':'Android & Media Server Developer'
				}
				]
			}) %>


			<!-- Skills area -->
			<%- include('modules/skillsArea',{
				<!-- Small title goes here -->
				smallTitle: 'Skills',

				<!-- Big title goes here -->
				bigTitle: 'Top languages and tools',

				<!-- Skill categories titles goes here -->
				skillsTitle: [
				'Programming Languages',
				'Tools',
				'Design',
				]
			}) %>

			<!-- Projects area -->
			<%- include('modules/projectsArea',{
				<!-- Small title goes here -->
				smallTitle: 'Projects',

				<!-- Big title goes here -->
				bigTitle: 'My recent work',

				<!-- Button text -->
				buttonText: 'VIEW MORE'
			}) %>


			<!-- Training area -->
			<%- include('modules/trainingArea',{
				<!-- Small title goes here -->
				smallTitle: 'Training',

				<!-- Big title goes here -->
				bigTitle: 'Certificates/Training',

				<!-- Education tiles goes here -->
				training: [
				'IC3 Digital Literacy Certification',
				'Erasmus exchange experience in 2016',
				'PHP Fundamentals course by Edwin Diaz',
				'The Complete Javascript course by Jonas Schmedtmann',
				'React course by Maximilian Schwarzmüller',
				'Docker Workshop by JSLeague',
				]
			}) %>

			<!-- Achivements area -->
			<%- include('modules/achivementArea',{
				<!-- Small title goes here -->
				smallTitle: 'Achivements',

				<!-- Big title goes here -->
				bigTitle: 'Achivements',

				<!-- Achivement tiles goes here -->
				achivement: [
				'2019</span><br> National Olympiad of Technology and Information<br>Bronze Medal',
				'2019</span><br> Micro:bit Article in Highschool\'s Magazine<br>\"ROaITă - Revista pentru pasionati de IT&C 4\"',
				]
			}) %>


			<!-- Contact area -->
			<%- include('modules/contactArea',{
				<!-- Small title goes here -->
				smallTitle: 'Contact',

				<!-- Big title goes here -->
				bigTitle: 'Get in touch with me',

				<!-- Location goes here -->
				location: 'Romania, Bucharest',

				<!-- Placeholders for inputs goes here -->
				placeholder: [
				'Email',
				'Subject',
				'Message',
				],

				<!-- Submit button text goes here -->
				submitButton: 'Send Message'
			}) %>

		</div>
	</div>
	<input type="hidden" class="languageDetect" value="<%= language %>">
	<script>
		tippy('.skillImage', {
			placement: 'bottom',
			zIndex: 100,
			animation: 'shift-toward',
			theme: 'light-border',
			touch: false
		});
	</script>
	<script src="index.js"></script>
</body>

</html>